class Main {
    function void main() 
    {
        var Drawer drawer;
        var Maze maze;
        var BFS bfs;
        
        let maze = Maze.new();
        do maze.Fill();
        let drawer = Drawer.new(maze);
        do Drawer.drawSideMenu(0);
        do Drawer.drawHelper();
        do drawer.drawMaze();
        let bfs = BFS.new(maze);
        do bfs.run();

        do bfs.dispose();
        do maze.dispose();
        do drawer.dispose();
        
        return;
    }
}
